.detail_third {
  width: 725rpx;
  margin: 0 auto;
  overflow: hidden;
}

/* 左 */
.detail_third_left {
  float: left;
  width: 430rpx;
  padding: 20rpx;
}

.detail_third_left_price {
  height: 50rpx;
  line-height: 50rpx;
  font-size: 42rpx;
  font-weight: 400;
  color: rgba(255, 0, 0, 1);
}

.detail_third_left_pricedesc {
  padding-left: 8rpx;
  line-height: 40rpx;
  height: 40rpx;
  font-size: 22rpx;
  font-weight: 400;
  color: rgba(255, 0, 0, 1);
}

/* 右 */
.detail_third_right {
  width: 270rpx;
  float: right;
  text-align: right;
  padding: 20rpx 10rpx 0 0;
}

.detail_third_right_goodsnum {
  height: 46rpx;
  overflow: hidden;
}

.detail_third_right_goodsnum_minus,
.detail_third_right_goodsnum_add,
.detail_third_right_goodsnum_num {
  float: right;
  width: 46rpx;
  height: 46rpx;
}

.detail_third_right_goodsnum_num {
  width: 105rpx;
  border-top: 1rpx solid rgba(200, 200, 200, 0.5);
  border-bottom: 1rpx solid rgba(200, 200, 200, 0.5);
  text-align: center;
  line-height: 46rpx;
  font-family: Adobe Heiti Std R;
  color: rgba(51, 51, 51, 1);
  font-size: 30rpx;
}

.detail_third_right_goodsnum_minus image,
.detail_third_right_goodsnum_add image {
  width: 46rpx;
  height: 46rpx;
}


.detail_third_right_dec {
  margin-top: 4rpx;
  line-height: 40rpx;
  height: 40rpx;
  font-size: 22rpx;
  font-weight: 400;
  color: rgba(255, 0, 0, 1)
}

.detail_third_right_dec text {
  color: #3792DF;
}

