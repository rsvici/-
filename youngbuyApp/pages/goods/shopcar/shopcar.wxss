.specstype {
  width: 100%;
  height: 100vh;
  position: fixed;
  background: rgba(0, 0, 0, 0.42);
  top: 0;
  left: 0;
}

.specstype_first {
  box-sizing: border-box;
  width: 100%;
  height: 600rpx;
  overflow-y: auto;
  background: rgba(255, 255, 255, 1);
  border-radius: 26rpx;
  position: absolute;
  left: 0;
  bottom: 105rpx;
  padding: 40rpx;
}


.specstype_first_goods {
  overflow: hidden;
}

.specstype_first_type {
  height: 45rpx;
  line-height: 60rpx;
  background: transparent;

}

/* 选择规格参数 */
.specstype_first_color {
  margin-top: 15rpx;
}

.specstype_first_color_title {
  height: 50rpx;
  line-height: 50rpx;
  font-size: 32rpx;
  color: rgba(51, 51, 51, 1);
}

.specstype_first_color_type {
  overflow: hidden;
}

.specstype_first_color_type view {
  float: left;
  background: rgba(245, 245, 245, 1);
  border-radius: 27rpx;
  height: 54rpx;
  line-height: 54rpx;
  font-size: 25rpx;
  font-weight: 500;
  color: rgba(112, 112, 112, 1);
  padding: 0 30rpx;
  margin: 10rpx 15rpx;
}

.specstype_first_color_type view.active {
  background: rgba(255, 0, 0, 1);
  color: #FFFFFF;
}

/* 关闭 */
.specstype_close {
  position: absolute;
  right: 41rpx;
  bottom: 620rpx;
  width: 50rpx;
  height: 50rpx;
}

.specstype_close image {
  display: block;
  width: 26rpx;
  height: 26rpx;
  float: right;
}


/* 确认 */
.specstype_second {
  position: absolute;
  width: 660rpx;
  height: 82rpx;
  line-height: 82rpx;
  bottom: 10rpx;
  left: 45rpx;
  background: rgba(255, 0, 0, 1);
  border-radius: 14rpx;
  font-size: 32rpx;
  color: #fff;
  text-align: center;
}


