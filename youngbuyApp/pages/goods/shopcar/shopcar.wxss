/* 新增滑动 */
.touch-item {
  overflow: hidden;
  position: relative;
}

.touch-item .pay_shoplist_shopitem_goodslist_goodsitem_content {
  -webkit-transition: all 0.4s;
  transition: all 0.4s;
  -webkit-transform: translateX(50px);
  transform: translateX(50px);
  margin-left: -50px
}

.touch-move-active .pay_shoplist_shopitem_goodslist_goodsitem_content,
.touch-move-active .del {
  -webkit-transform: translateX(0);
  transform: translateX(0);
}

.del {
  position: absolute;
  right: 0rpx;
  top: 46rpx;
  font-size: 28rpx;
  width: 80rpx;
  height: 130rpx;
  text-align: center;
  line-height: 130rpx;
  border-radius: 7rpx;
  font-weight: 500;
  color: #fff;
  background-color: #ff0000;
  text-align: center;
  -webkit-transform: translateX(50px);
  transform: translateX(50px);
  -webkit-transition: all 0.4s;
  transition: all 0.4s;
}