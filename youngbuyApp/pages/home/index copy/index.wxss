/**index.wxss**/
.home_first {
  width: 750rpx;
  /* height: 502.57rpx; */
  background-image: url(http://www.appsun.com.cn/www/yb/home/bg.png);
  background-repeat: no-repeat;
  background-size: cover;
  background-position: 50%;
}

/* 头部搜索 */
.home_first_title {
  height: 100rpx;
  padding: 20rpx 35rpx;
}

.home_first_title>view {
  float: left;
  margin-left: 36rpx;
}

.home_first_title>view.home_first_title_sort {
  margin-top: 15rpx;
  margin-left: 0rpx;
  width: 42rpx;
  height: 42rpx;
}

.home_first_title_sort image {
  width: 42rpx;
  height: 42rpx;
}

.home_first_title_search {
  width: 452rpx;
  height: 72rpx;
}

.home_first_title_search image {
  width: 452rpx;
  height: 72rpx;
}

.home_first_title_camera {
  margin-top: 19rpx;
  width: 40rpx;
  height: 34rpx;
}

.home_first_title_camera image {
  display: block;
  width: 40rpx;
  height: 34rpx;
}

.home_first_title_news {
  margin-top: 14rpx;
  width: 44rpx;
  height: 44rpx;
}

.home_first_title_news image {
  width: 44rpx;
  height: 44rpx;
}

/* banner */
.home_first_banner {
  width: 698rpx;
  height: 335rpx;
  margin: 10rpx auto;
}

.home_first_banner swiper {
  width: 698rpx;
  height: 335rpx;
  overflow: hidden;
  border-radius: 14rpx;
}

.home_first_banner image {
  width: 698rpx;
  height: 335rpx;
}

