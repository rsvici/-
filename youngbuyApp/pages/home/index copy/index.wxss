/* 第二部分 */
/* 第二部分 */
/* 第二部分 */
.home_second {
  overflow: hidden;
  width: 700rpx;
  margin: 60rpx auto 0;
}

/* 显示抢购 */
.home_second_left {
  width: 340rpx;
  height: 349rpx;
  background: rgba(249, 249, 249, 1);
  border-radius: 14rpx;
  float: left;
}

.home_second_left_title {
  height: 50rpx;
  line-height: 70rpx;
  font-size: 31rpx;
  font-weight: 400;
  color: rgba(51, 51, 51, 1);
  text-align: center;
}

.home_second_left_time {
  text-align: center;
  height: 50rpx;
  line-height: 50rpx;
  font-size: 19rpx;
  font-weight: 400;
  color: rgba(254, 87, 82, 1);
}

.home_second_left_time text {
  display: inline-block;
  width: 36rpx;
  height: 31rpx;
  line-height: 31rpx;
  border-radius: 7rpx;
  font-size: 17rpx;
  font-weight: 400;
  color: rgba(255, 255, 255, 1);
  background: linear-gradient(180deg, rgba(254, 132, 105, 1), rgba(254, 87, 82, 1));
}

/* 秒杀banner */
.home_second_left_timeImg {
  height: 153rpx;
  width: 340rpx;
  margin: 0 auto;
}

.home_second_left_timeImg swiper {
  height: 240rpx;
  width: 340rpx;
}

.home_second_left_timeImg_img {
  height: 153rpx;
  width: 200rpx;
  margin: 0 auto;
  background-size: contain;
  background-repeat: no-repeat;
  background-position: 50%;
}

.home_second_left_timeImg_title {
  text-align: center;
  font-size: 25rpx;
  padding: 0 10rpx;
  font-weight: 400;
  color: rgba(51, 51, 51, 1);
  height: 50rpx;
  line-height: 65rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.home_second_left_timeImg_price {
  text-align: center;
  height: 40rpx;
  line-height: 35rpx;
}

.home_second_left_timeImg_price_1 {
  font-size: 28rpx;
  font-family: MicrosoftYaHei;
  font-weight: 400;
  color: rgba(255, 0, 0, 1);
  margin-right: 5rpx;
}

.home_second_left_timeImg__2 {
  font-size: 24rpx;
  font-weight: 400;
  text-decoration: line-through;
  color: rgba(112, 112, 112, 1);
}


/* 右边 */
.home_second_right {
  width: 340rpx;
  margin-left: 19rpx;
  float: left;
}

/* 拼团 */
.home_second_right_first {
  width: 340rpx;
  height: 153rpx;
  background: rgba(249, 249, 249, 1);
  border-radius: 14rpx;
  padding: 25rpx 10rpx 0;
  position: relative;
}

.home_second_right_first_tab {
  position: absolute;
  top: 0;
  right: 0;
  width: 76rpx;
  height: 39rpx;
}

.home_second_right_first_tab image {
  width: 76rpx;
  height: 39rpx;
}

.home_second_right_first swiper {
  width: 320rpx;
  height: 153rpx;
}

.home_second_right_first_item_img {
  height: 80rpx;
  width: 80rpx;
  margin: 0 auto;
  background-size: contain;
  background-repeat: no-repeat;
  background-position: 50%;
}

.home_second_right_first_item_price {
  height: 40rpx;
  line-height: 50rpx;
  font-size: 20rpx;
  font-weight: 400;
  color: rgba(255, 0, 0, 1);
  text-align: center;
}





/* 预售 */
.home_second_right_second {
  width: 340rpx;
  margin-top: 19rpx;
  height: 179rpx;
  padding-top: 15rpx;
  background: rgba(249, 249, 249, 1);
  border-radius: 14rpx;
  position: relative;
}

.home_second_right_second_tab {
  position: absolute;
  top: 0;
  left: 0;
  width: 76rpx;
  height: 39rpx;
}

.home_second_right_second_tab image {
  width: 76rpx;
  height: 39rpx;
}

.home_second_right_second swiper {
  width: 300rpx;
  height: 150rpx;
  margin: 0 auto;
}

.home_second_right_second_item_img {
  float: right;
  width: 103rpx;
  height: 148rpx;
  margin-right: 10rpx;
  background-size: contain;
  background-repeat: no-repeat;
  background-position: 50%;
}

.home_second_right_second_item_left {
  float: left;
  width: 190rpx;
  height: 148rpx;
  margin-top: 30rpx;
}

.home_second_right_second_item_name {
  text-align: center;
  font-size: 25rpx;
  padding: 0 10rpx;
  font-weight: 400;
  color: rgba(51, 51, 51, 1);
  height: 80rpx;
  line-height: 40rpx;
  text-overflow: -o-ellipsis-lastline;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  align-content: center;
}

.home_second_right_second_item_price {
  height: 40rpx;
  line-height: 40rpx;
  font-size: 25rpx;
  font-weight: 400;
  color: rgba(255, 0, 0, 1);
  text-align: center;
}

/* 第三部分new */
.home_third {
  margin-top: 60rpx;
}

.home_third_title {
  text-align: center;
}

.home_third_title image {
  width: 76rpx;
  height: 26rpx;
}

.home_third_box {
  width: 699rpx;
  height: 321rpx;
  background: rgba(255, 255, 255, 1);
  box-shadow: 0rpx 0rpx 14rpx 0px rgba(0, 0, 0, 0.1);
  border-radius: 7rpx;
  margin: 20rpx auto;
}

.home_third_box swiper {
  width: 699rpx;
  height: 321rpx;
}

.home_third_box_item_img {
  margin: 25rpx auto 20rpx;
  width: 200rpx;
  height: 155rpx;
  background-size: contain;
  background-repeat: no-repeat;
  background-position: 50%;
}

.home_third_box_item_name {
  text-align: center;
  font-size: 25rpx;
  padding: 0 20rpx;
  font-weight: 400;
  color: rgba(51, 51, 51, 1);
  height: 80rpx;
  line-height: 40rpx;
  text-overflow: -o-ellipsis-lastline;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  align-content: center;
}

.home_third_box_item_price {
  height: 30rpx;
  line-height: 30rpx;
  font-size: 25rpx;
  font-weight: 400;
  color: rgba(255, 0, 0, 1);
  text-align: center;
}

/* 第四部分 like */
.home_fourth_title {
  text-align: center;
  margin-top: 60rpx;
}

.home_fourth_title image {
  width: 180rpx;
  height: 31rpx;
}

.home_fourth_box {
  padding: 5rpx;
  overflow: hidden;
}

.like_item {
  float: left;
  margin: 10rpx;
  width: 350rpx;
  margin-top: 20rpx;
  background-color: #fff;
  position: relative;
  border-radius: 10rpx;
}

.like_item_left {
  position: relative;
  width: 355rpx;
  height: 300rpx;
  border-radius: 14rpx;
  text-align: center;
  padding-top: 22.5rpx;
}

.like_item_left>image {
  width: 300rpx;
  height: 270rpx;
}

.like_item_left_nav {
  position: absolute;
  top: 0;
  right: 0;
}

.like_item_left_nav1 {
  position: absolute;
  top: 10rpx;
  left: 10rpx;
}

.like_item_left_nav.ms image {
  width: 106rpx;
  display: block;
}

.like_item_left_nav.pt image {
  width: 41rpx;
  display: block;
}

.like_item_left_nav1.ys image {
  width: 56rpx;
  display: block;
}

.like_item_right {
  padding: 20rpx;
}

.like_item_right_name {
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  font-size: 30rpx;
  color: #000;
}

.like_item_right_specs {
  font-size: 24rpx;
  height: 60rpx;
  line-height: 60rpx;
  color: rgba(90, 90, 90, 1);
}

.like_item_right_specs image {
  display: inline-block;
  width:28rpx;
  height:25.5rpx;
  vertical-align: -1rpx;
}


.like_item_right_price {
  height: 50rpx;
  line-height: 65rpx;
  color: rgba(255, 0, 0, 1);
  font-size: 28rpx;
}

/* 按钮 */
.like_item_btn {
  position: absolute;
  bottom: 25rpx;
  right: 25rpx;
  width: 200rpx;
  height: 60rpx;
  /* background-color: #FE1717; */
}

.like_item_btn_shopcar {
  position: absolute;
  bottom: 10rpx;
  right: 15rpx;
  width: 47rpx;
}

.like_item_btn_shopcar image {
  width: 47rpx;
}