/* 第二部分 */
/* 第二部分 */
.home_second {
  overflow: hidden;
  width: 750rpx;
  margin: 25rpx auto;
  position: relative;
}

/* 显示抢购 */
.home_second_left {
  width: 750rpx;
  height: 450rpx;
  border-radius: 14rpx;
}

.home_second_left_title {
  height: 50rpx;
  line-height: 70rpx;
  font-size: 31rpx;
  font-weight: 400;
  color: rgba(51, 51, 51, 1);
  text-align: center;
}

.home_second_left_time {
  text-align: center;
  height: 70rpx;
  line-height: 70rpx;
  font-size: 19rpx;
  font-weight: 400;
  color: rgba(254, 87, 82, 1);
}

.home_second_left_time text {
  display: inline-block;
  width: 36rpx;
  height: 31rpx;
  line-height: 31rpx;
  border-radius: 7rpx;
  font-size: 17rpx;
  font-weight: 400;
  color: rgba(254, 87, 82, 1);
  border: 1px solid rgba(254, 87, 82, 1);
}

/* 秒杀banner */
.home_second_left_timeImg {
  width: 699rpx;
  height: 308rpx;
  background: rgba(255, 255, 255, 1);
  box-shadow: 0rpx 0rpx 20rpx 0rpx rgba(0, 0, 0, 0.1);
  border-radius: 7rpx;
  margin: 0 auto;
}

.home_second_left_timeImg swiper {
  height: 308rpx;
  width: 600rpx;
}

.home_second_left_timeImg_img {
  height: 160rpx;
  width: 200rpx;
  margin: 20rpx auto 0;
  background-size: contain;
  background-repeat: no-repeat;
  background-position: 50%;
}

.home_second_left_timeImg_title {
  text-align: center;
  font-size: 25rpx;
  padding: 0 20rpx;
  font-weight: 400;
  color: rgba(51, 51, 51, 1);
  height: 60rpx;
  line-height: 75rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.home_second_left_timeImg_price {
  text-align: center;
  height: 40rpx;
  line-height: 35rpx;
}

.home_second_left_timeImg_price_1 {
  font-size: 28rpx;
  font-family: MicrosoftYaHei;
  font-weight: 400;
  color: rgba(255, 0, 0, 1);
  margin-right: 5rpx;
}

.home_second_left_timeImg__2 {
  font-size: 24rpx;
  font-weight: 400;
  text-decoration: line-through;
  color: rgba(112, 112, 112, 1);
}

.home_second_right {
  position: absolute;
  right: 45rpx;
  top: 200rpx;

}

.home_second_right image {
  width: 80rpx;
}

